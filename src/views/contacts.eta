<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Contacts</title>
</head>
<body>
  <h1>Contacts</h1>
  <% if (it.contacts && it.contacts.length > 0) { %>
    <ul>
      <% it.contacts.forEach(contact => { %>
        <li><%= contact.name %> - <%= contact.email %></li>
      <% }) %>
    </ul>
  <% } else { %>
    <p>No contacts found.</p>
  <% } %>
</body>
</html>
